    "prop-types": "^15.5.6",
    "react-bootstrap": "^0.32.3",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "redux-logger": "^2.7.4",
    "style-loader": "^0.13.0",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^2.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.256.1",
    "axios": "^0.18.0",
