{
  "name": "influent-dashboard",
  "version": "0.6.0",
  "description": "Dashboard system for Influent Metrics products",
  "author": "Influent Metrics",
  "license": "ISC",
  "homepage": "https://influentmetrics.com/",
  "main": "index-personalizer.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run importer",
    "personalizer": "set NODE_ENV=dev&& forever start --pidPath ./log --pidFile personalizer.pid -p . -a -l log/ui-personalizer.log -o log/ui-personalizer.out.log -e log/ui-personalizer.err.log --workingDir . ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base build/ --config ui/webpack-personalizer.config.js",
    "importer": "set NODE_ENV=dev&& forever start --pidPath ./log --pidFile importer.pid -p . -a -l log/ui-importer.log -o log/ui-importer.out.log -e log/ui-importer.err.log --workingDir . ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base build/ --config ui/webpack-importer.config.js",
    "build": "rm -rf ./build && NODE_ENV=\"production\" webpack",
    "lint-break-on-errors": "eslint ./source/js ./webpack.config.js -f table --ext .js --ext .jsx",
    "lint": "eslint ./components/common ./webpack.config.js -f table --ext .js --ext .jsx",
    "preview": "NODE_ENV=\"production\" ./node_modules/webpack-dashboard/bin/webpack-dashboard.js -t 'Preview Mode - Influent Dashboard' -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove",
    "si": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base build/ --config ui/webpack-importer.config.js",
    "api": "forever start --pidPath ./log --pidFile api.pid -p . -a -w -l log/api.log -o log/api.out.log -e log/api.err.log --watchDirectory api --workingDir api api/test.js",
    "http": "forever start --pidPath ./log --pidFile http.pid -p . -a -w -l log/http.log -o log/http.out.log -e log/http.err.log --watchDirectory http --workingDir http http/server.js",
    "all": "npm run api & npm run http & npm run importer & npm run personalizer",
    "stopall": "forever stopall"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.6.1",
    "css-loader": "^0.14.5",
    "draft-js": "^0.10.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^2.24.1",
    "jasny-bootstrap": "^3.1.3",
    "postcss-loader": "^1.1.1",
